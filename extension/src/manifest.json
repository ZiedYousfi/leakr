{
  "manifest_version": 3,
  "name": "Leakr",
  "version": "1.1",
  "description": "Find leaks of your favorite streamers ;)",
  "action": {
    "default_popup": "src/popup.html"
  },
  "permissions": ["tabs"],
  "icons": {
    "48": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
