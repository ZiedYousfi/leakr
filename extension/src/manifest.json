{
  "manifest_version": 3,
  "name": "Leakr",
  "version": "1.2.0",
  "description": "Find leaks of your favorite streamers ;)",
  "action": {
    "default_popup": "src/popup.html"
  },
  "permissions": ["tabs", "storage", "scripting"],
  "host_permissions": [ "<all_urls>" ],
  "icons": {
    "48": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
